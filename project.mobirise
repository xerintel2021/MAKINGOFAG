        {
          "_name": "map2",
          "_customHTML": "<section class=\"form map2 therapym4_map2\" group=\"Form\" plugins=\"formstyler, datepicker\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" />\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" />\n        <header>Map</header>\n        <input type=\"map\" title=\"Map\" name=\"googleMap\" />\n        <header>Style</header>\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\" checked=\"@ADDONS_PATH@/therapym4/components/>\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\" />\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#eceae4\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#eceae4\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"fieldDrop\" title=\"Field Dropdown\" value=\"#f6f3ee\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#f6f3ee\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#f6f3ee\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#000000\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"formFocusText\" title=\"Text Focus\" value=\"#000000\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#d58f76\" condition=\"customStyle\" />\n        <input type=\"color\" name=\"formFocusPlaceholder\" title=\"Placeholders Focus\" value=\"#d58f76\"\n            condition=\"customStyle\" />\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@ADDONS_PATH@/therapym4/components/assets/background.jpg\" />\n            <input type=\"color\" title=\"Background Color\" value=\"#f6f3ee\" selected=\"@ADDONS_PATH@/therapym4/components/>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"@ADDONS_PATH@/therapym4/components/>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\"\n            condition=\"overlay &amp;&amp; bg.type !== 'color'\" />\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\"\n            condition=\"overlay &amp;&amp; bg.type !== 'color'\" />\n        <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\" />\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-lg-6 map-col col-md-12\">\n                <div class=\"google-map\" mbr-map=\"googleMap\"></div>\n            </div>\n\n            <div mbr-form=\"\" class=\"col-lg-6 col-md-12 mbr-form\">\n                <!--Formbuilder Form-->\n                <form action=\"\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                    <div class=\"form-row\">\n                        <div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">\n                            Thanks for filling out the form!\n                        </div>\n                        <div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">\n                            Oops...! some problem!\n                        </div>\n                    </div>\n                    <div class=\"dragArea form-row\">\n                        <div class=\"col-lg-12 col-md-12 col-sm-12\">\n                            <h6 mbr-theme-style=\"display-4\" class=\"mbr-section-subtitle align-left mbr-fonts-style\">\n                                CONTACT US\n                            </h6>\n                        </div>\n                        <div class=\"col-lg-12 col-md-12 col-sm-12\">\n                            <h5 mbr-theme-style=\"display-2\" class=\"mbr-bold align-left mbr-fonts-style pb-5\">\n                                Welcome to counseling\n                            </h5>\n                        </div>\n                        <div data-for=\"name\" class=\"col-lg-6 col-md-12 col-sm-12 form-group\">\n                            <input type=\"text\" name=\"name\" placeholder=\"Name\" data-form-field=\"name\"\n                                mbr-theme-style=\"display-7\" class=\"form-control\" value=\"@ADDONS_PATH@/therapym4/components/>\n                        </div>\n                        <div data-for=\"email\" class=\"col-lg-6 col-md-12 col-sm-12 form-group\">\n                            <input type=\"email\" name=\"email\" placeholder=\"E-mail\" data-form-field=\"email\"\n                                mbr-theme-style=\"display-7\" class=\"form-control\" value=\"@ADDONS_PATH@/therapym4/components/>\n                        </div>\n                        <div data-for=\"phone\" class=\"col-lg-6 col-md-12 col-sm-12 form-group\">\n                            <input type=\"url\" name=\"phone\" placeholder=\"Phone\" data-form-field=\"url\"\n                                mbr-theme-style=\"display-7\" class=\"form-control\" value=\"@ADDONS_PATH@/therapym4/components/>\n                        </div>\n                        <div data-for=\"select\" class=\"col-lg-6 col-md-12 col-sm-12 form-group\">\n                            <select name=\"select\" data-form-field=\"select\" mbr-theme-style=\"display-7\"\n                                class=\"form-control\">\n                                <option value=\"Group\">Group</option>\n                                <option value=\"Individual\">Individual</option>\n                            </select>\n                        </div>\n                        <div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"textarea\">\n                            <textarea name=\"textarea\" placeholder=\"Message\" data-form-field=\"textarea\"\n                                mbr-theme-style=\"display-7\" class=\"form-control\"></textarea>\n                        </div>\n                        <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\"\n                            data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\"\n                            class=\"col-lg-12 col-md-12 col-sm-12 align-left\">\n                            <a data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" type=\"submit\"\n                                class=\"btn btn-primary\">\n                                SEND\n                            </a>\n                        </div>\n                    </div>\n                </form>\n                <!--Formbuilder Form-->\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            ".mbr-overlay": {
              "background-color": "@overlayColor",
              "opacity": "@overlayOpacity"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "input, textarea, .jqselect": {
              "font-weight": "300 !important"
            },
            "textarea": {
              "min-height": "200px !important"
            },
            ".map-col": {
              "border-radius": "50%",
              "overflow": "hidden",
              "width": "35rem",
              "height": "35rem",
              "margin": "auto"
            },
            "@media (max-width: 992px)": {
              ".map-col": {
                "margin-bottom": "4rem"
              }
            },
            ".google-map": {
              "height": "35rem",
              "width": "35rem",
              "margin": "auto",
              "border-radius": "50%",
              "overflow": "hidden",
              "position": "relative",
              "iframe": {
                "height": "100%",
                "width": "100%"
              },
              "[data-state-details]": {
                "color": "#6b6763",
                "font-family": "Montserrat",
                "height": "1.5em",
                "margin-top": "-0.75em",
                "padding-left": "1.25rem",
                "padding-right": "1.25rem",
                "position": "absolute",
                "text-align": "center",
                "top": "50%",
                "width": "100%"
              },
              "&[data-state]": {
                "background": "#e9e5dc"
              },
              "&[data-state='loading']": {
                "[data-state-details]": {
                  "display": "none"
                },
                "&::after": {}
              }
            },
            "@media (max-width: 767px)": {
              ".map-col, .google-map": {
                "height": "20rem",
                "border-radius": "0",
                "width": "auto",
                "padding": "0"
              }
            },
            "& when (@customStyle)": {
              ".jq-selectbox__select-text": {
                "color": "@formPlaceholder"
              },
              ".jq-selectbox li": {
                "background-color": "@fieldDrop !important",
                "padding": "1.2em 1.4em !important",
                "color": "@formPlaceholder !important",
                "border-top": "1px solid @fieldColor"
              },
              ".form-control, .field-input": {
                "padding": "1.1rem 1.5rem",
                "height": "auto",
                "background-color": "@fieldColor",
                "border-color": "@borderColor",
                "color": "@formText",
                "transition": "0.4s",
                "box-shadow": "none",
                "outline": "none",
                "&::-webkit-input-placeholder, &::-webkit-input-placeholder": {
                  "color": "@formPlaceholder"
                },
                "&:-moz-placeholder, &:-moz-placeholder": {
                  "color": "@formPlaceholder"
                },
                "&:hover, &:focus": {
                  "background-color": "@fieldHover",
                  "border-color": "@borderHover !important",
                  "color": "@formFocusText",
                  "box-shadow": "none",
                  "outline": "none",
                  "&::-webkit-input-placeholder, &::-webkit-input-placeholder": {
                    "color": "@formFocusPlaceholder"
                  },
                  "&:-moz-placeholder, &:-moz-placeholder": {
                    "color": "@formFocusPlaceholder"
                  }
                }
              },
              "textarea": {
                "padding-top": "1rem !important"
              },
              ".jq-number__spin:hover, .jq-number__spin:focus": {
                "background-color": "@fieldHover",
                "border-color": "@borderHover",
                "color": "contrast(@formText, #000000, #ffffff, 30%)",
                "box-shadow": "none",
                "outline": "none"
              },
              ".jq-number__spin": {
                "background-color": "@fieldColor",
                "border-color": "@borderColor",
                "color": "@formText",
                "transition": "0.4s",
                "box-shadow": "none",
                "outline": "none"
              },
              ".jq-selectbox li, .jq-selectbox li": {
                "background-color": "@fieldColor",
                "color": "contrast(@fieldColor, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox li:hover, .jq-selectbox li.selected": {
                "background-color": "@fieldHover",
                "color": "contrast(@fieldHover, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox:hover .jq-selectbox__trigger-arrow, .jq-number__spin.minus:hover:after, .jq-number__spin.plus:hover:after": {
                "border-top-color": "@fieldColor",
                "border-bottom-color": "@fieldColor"
              },
              ".jq-selectbox .jq-selectbox__trigger-arrow, .jq-number__spin.minus:after, .jq-number__spin.plus:after": {
                "border-top-color": "@fieldHover",
                "border-bottom-color": "@fieldHover"
              },
              ".jq-selectbox__trigger-arrow": {
                "border-top-color": "@formPlaceholder !important",
                "border-bottom-color": "@formPlaceholder !important"
              }
            },
            "& when (@customStyle), (@disableElements)": {
              "input::-webkit-clear-button": {
                "display": "none"
              },
              "input::-webkit-inner-spin-button": {
                "display": "none"
              },
              "input::-webkit-outer-spin-button": {
                "display": "none"
              },
              "input::-webkit-calendar-picker-indicator": {
                "display": "none"
              }
            },
            "H5": {
              "color": "#200960"
            },
            "H6": {
              "color": "#d58f76"
            }
          },
