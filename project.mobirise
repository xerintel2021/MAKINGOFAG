          "_customHTML": "<section class=\"features3\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Ancho completo\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Parte superior\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Fondo\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Title Icon\" name=\"showIcon\">\n        <input type=\"range\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"8\" step=\"1\" value=\"5\">\n\n\n\n\n\n        <header>Color</header>\n        <input type=\"color\" title=\"Title Icon\" name=\"iconColor2\" value=\"#59bb93\" selected condition=\"showIcon\">\n        <input type=\"color\" title=\"Icon\" name=\"iconColor\" value=\"#1d6ba1\">\n        <input type=\"color\" title=\"Icon BG\" name=\"cardColor\" value=\"#ffffff\">\n\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background8.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Superposición\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Color de superposición\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacidad\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"title col-12 col-md-12 col-lg-6\">\n                <span mbr-icon class=\" icon mobi-mbri-paper-plane mobi-mbri pb-4\" mbr-if=\"showIcon\"></span>\n                <div class=\"title-box pb-4\" mbr-if=\"showTitle\">\n                    <h4 class=\"main-title align-left mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".main-title\"><b>Across a Wide Range\n                            <br>of Categories</b></h4>\n                </div>\n            </div>\n        </div>\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-3 col-lg-3\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-box align-center\">\n\n                        <a href=\"bandeja-entrada-talento.html\"><span mbr-icon class=\"mbr-iconfont icon54-v1-wechat\"></span></a>\n\n                        <h4 class=\"card-title align-center mbr-semibold mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title, .card-box\">\n                            Chat\n                        </h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 col-lg-3\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-box align-center\">\n                        <a href=\"candidaturas-talento.html\"><span mbr-icon class=\"mbr-iconfont icon54-v1-film-clapper\"></span></a>\n                        <h4 class=\"card-title align-center mbr-semibold mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title, .card-box\">\n                            Candidaturas\n                        </h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 col-lg-3\" mbr-if=\"cardsAmount > 2\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-box align-center\">\n                        <a href=\"proyectos-finalizados.html\"><span mbr-icon class=\"mbr-iconfont icon54-v1-done-bag\"></span></a>\n                        <h4 class=\"card-title align-center mbr-semibold mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title, .card-box\">Proyectos&nbsp;<div>Finalizados</div></h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 col-lg-3\" mbr-if=\"cardsAmount > 3\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-box align-center\">\n                        <a href=\"editar-perfil.html\"><span mbr-icon class=\"mbr-iconfont icon54-v1-ball-pen\"></span></a>\n                        <h4 class=\"card-title align-center mbr-semibold mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title, .card-box\">Editar Perfil</h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 col-lg-3\" mbr-if=\"cardsAmount > 4\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-box align-center\">\n                        <a href=\"notificaciones.html\"><span mbr-icon class=\"mbr-iconfont mobi-mbri-alert mobi-mbri\"></span></a>\n                        <h4 class=\"card-title align-center mbr-semibold mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title, .card-box\">Notificaciones</h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 col-lg-3\" mbr-i